package com.mycompany.multiplicaxion;

/**
 *
 * @author ANGIE BARON
 */
public class operaciones {
    
 public static int sumar(int num1, int num2) {
        return num1 + num2;
    }
    
    public static int restar(int num1, int num2) {
        return num1 - num2;
    }
    
    public static int multiplicar(int num1, int num2) {
        return num1 * num2;
    }
}
